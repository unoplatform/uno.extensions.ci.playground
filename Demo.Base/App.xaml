<Application x:Class="Demo.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:wasm="http://platform.uno/wasm"
			 xmlns:local="using:Demo"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 mc:Ignorable="wasm">

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<!-- Load WinUI resources -->
				<XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
				<MaterialColors xmlns="using:Uno.Material" />
								<!-- Temporarily removing due to issue in Uno.Material-->
								<!--OverrideSource="ms-appx:///Demo/Styles/ColorPaletteOverride.xaml" />-->
				<MaterialFonts xmlns="using:Uno.Material" />
								<!--OverrideSource="ms-appx:///Demo/Styles/MaterialFontsOverride.xaml" />-->
				<MaterialResources xmlns="using:Uno.Material" />

				<!-- Load Uno.UI.Toolkit resources -->
				<ToolkitResources xmlns="using:Uno.Toolkit.UI" />
				<MaterialToolkitResources xmlns="using:Uno.Toolkit.UI.Material" />
			</ResourceDictionary.MergedDictionaries>
			<!-- Add resources here -->
		</ResourceDictionary>
	</Application.Resources>

</Application>
